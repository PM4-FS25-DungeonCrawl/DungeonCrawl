        <type>void</type>
        <definition>void log_msg</definition>
        <argsstring>(log_level_t level, const char *module, const char *format,...)</argsstring>
        <name>log_msg</name>
        <param>
          <type>log_level_t</type>
          <declname>level</declname>
        </param>
        <param>
          <type>const char *</type>
          <declname>module</declname>
